# Frontend Environment Variables
VITE_BACKEND_URL=http://localhost:8080
VITE_WS_URL=ws://localhost:8080/ws

# Nostr Relay URLs (comma-separated)
VITE_NOSTR_RELAYS=wss://relay.damus.io,wss://nos.lol,wss://relay.nostr.band

# IPFS Gateway
VITE_IPFS_GATEWAY=https://ipfs.io

# App Configuration
VITE_APP_NAME=Nostr Delivery
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_LIGHTNING=true
VITE_ENABLE_IPFS=false
VITE_ENABLE_GPS=false

# Development
VITE_DEV_MODE=true
